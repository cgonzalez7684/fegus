				<xs:element ref="SaldoProductosPorCobrar" minOccurs="1"/>
				<xs:element ref="CuentaContablePorDesembolsarConCompromiso" minOccurs="0"/>
				<xs:element ref="SaldoPorDesembolsarConCompromiso" minOccurs="1"/>
