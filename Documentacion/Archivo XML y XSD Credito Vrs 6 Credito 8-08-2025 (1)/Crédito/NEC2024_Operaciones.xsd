				<xs:element ref="IdOperacionCredito" minOccurs="1"/>
				<xs:element ref="IdLineaCredito" minOccurs="0" />
				<xs:element ref="IndicadorOperacionModificada" minOccurs="1"/>
				<xs:element ref="IndicadorPresentaCodeudores" minOccurs="1"/>
				<xs:element ref="TipoEnfoque" minOccurs="1"/>
				<xs:element ref="TipoSegmento" minOccurs="1"/>
				<xs:element ref="CodigoEtapa" minOccurs="1"/>
				<xs:element ref="CodigoCategoriaRiesgo" minOccurs="1"/>
				<xs:element ref="TasaIncumplimiento" minOccurs="1"/>
				<xs:element ref="LGDMinimoCombinado" minOccurs="0"/>
				<xs:element ref="LGDPromedio" minOccurs="0"/>
				<xs:element ref="LGDRegulatorio" minOccurs="0"/>
				<xs:element ref="TipoOperacion" minOccurs="1"/>
				<xs:element ref="TipoCatalogoSUGEF" minOccurs="1"/>
				<xs:element ref="CodigoPaisDestinoCredito" minOccurs="0"/>
				<xs:element ref="CodigoProvinciaDestinoCredito" minOccurs="0"/>
				<xs:element ref="CodigoCantonDestinoCredito" minOccurs="0"/>
				<xs:element ref="CodigoDistritoDestinoCredito" minOccurs="0"/>
				<xs:element ref="CodigoProvinciaDependenciaCredito" minOccurs="0"/>
				<xs:element ref="CodigoCantonDependenciaCredito" minOccurs="0"/>
				<xs:element ref="TipoCarteraCrediticia" minOccurs="1"/>
				<xs:element ref="TipoEstadoOperacionCrediticia" minOccurs="0"/>
				<xs:element ref="DiasMaximaMorosidad" minOccurs="1"/>
				<xs:element ref="MontoFormalizadoOperacionCrediticia" minOccurs="0"/>
				<xs:element ref="TipoMonedaOperacion" minOccurs="1"/>
				<xs:element ref="MontoOperacionAutorizado" minOccurs="1"/>
				<xs:element ref="CuentaContablePrincipal" minOccurs="0"/>
				<xs:element ref="SaldoPrincipalOperacionCrediticia" minOccurs="1"/>
				<xs:element ref="CuentaContablePrincipalConDepositoPrevio" minOccurs="0"/>
				<xs:element ref="SaldoPrincipalConDepositoPrevio" minOccurs="1"/>
