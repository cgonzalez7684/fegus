				<xs:element ref="TipoCuotaPrincipal" minOccurs="0"/>
				<xs:element ref="MontoCuotaPrincipalActual" minOccurs="0"/>
				<xs:element ref="MontoCuotaInteresesActual" minOccurs="0"/>
