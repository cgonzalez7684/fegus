				<xs:element ref="IndicadorFormaPagoVigentePrincipal" minOccurs="0"/>
				<xs:element ref="IndicadorFormaPagoVigenteIntereses" minOccurs="0"/>
				<xs:element ref="FechaCorteOperacion" minOccurs="0"/>
				<xs:element ref="FechaProximoPagoPrincipal" minOccurs="0"/>
				<xs:element ref="FechaProximoPagoIntereses" minOccurs="0"/>
				<xs:element ref="FechaAmortizacionHasta" minOccurs="0"/>
				<xs:element ref="FechaInteresHasta" minOccurs="0"/>
				<xs:element ref="FechaPagoPactadoPrincipal" minOccurs="0"/>
				<xs:element ref="FechaPagoPactadoIntereses" minOccurs="0"/>
